#: import ScrollEffect kivy.effects.scroll.ScrollEffect
<Image1@ButtonBehavior+Image>:
<ItemDrawer>:
    theme_text_color: 'Primary'
    on_release: self.parent.set_color_item(self)

    IconLeftWidget:
        id: icon
        icon: root.icon
        theme_text_color: 'Secondary'
        text_color: root.text_color
<ContentNavigationDrawer>:
    orientation: "vertical"
    
    MDCard:
        orientation: "vertical"
        size_hint_y:None
        height:"170dp"
        spacing:5
        Image1:
            id:loger
            source:'image/shortcutkey.png'
            size_hint_y:None
            height:"150dp"
            
        MDLabel:
            id:user_id
            text:"Version 1.5.30"
            markup:True
            font_size:"10dp"
                        
            text_size:self.size
            
            halign:'right'
            valign:'top'
            

    BoxLayout:

        ScrollView:
            effect_cls: ScrollEffect
            canvas.before:
                Color:
                    rgba:[1,1,1,1]
                Rectangle:
                    size:self.size
                    pos:self.pos


            DrawerList:
                
                id: md_list
                Label:
                    height:"20dp"
                MDSeparator:
                OneLineAvatarListItem:
                    text: "Languages and Founder"
                    on_release:root.notebook("lanc")
                    IconLeftWidget:
                        icon: "code-braces"
                MDSeparator:
                OneLineAvatarListItem:
                    text: "Memory Unit"
                    on_release:root.notebook("memo")
                    IconLeftWidget:
                        icon: "memory"
                MDSeparator:
                OneLineAvatarListItem:
                    text: "File and Extension"
                    on_release:root.notebook("fileex")
                    IconLeftWidget:
                        icon: "file-delimited-outline"
                MDSeparator:
                OneLineAvatarListItem:
                    text: "Abbreviations"
                    on_release:root.notebook("abbre")
                    IconLeftWidget:
                        icon: "xml"
                MDSeparator:
                OneLineAvatarListItem:
                    text: "Command Windows"
                    on_release:root.notebook("cmdc")
                    IconLeftWidget:
                        icon: "powershell"
                MDSeparator:
                
                Label:
                    size_hint_y:None
                    height:"30dp"
                    
                MDLabel:
                    text:"**ALL THE BEST**"
                    markup:True
                    pos_hint: {"center_x": .5, "center_y": .5}
                    theme_text_color: "Custom"
                    bold:True
                    halign:'center'
                    text_color:[0,0,0,1]
                            
<HomePage>:
    
    name:'HomePage'
    
    MDNavigationLayout:
              
                            
                            

                
                    
        
        
                
        ScreenManager:
            Screen:            
                BoxLayout:
                    orientation: 'vertical'
                    canvas.before:
                        Color:
                            rgba:[1,1,1,1]
                        Rectangle:
                        
                            size:self.size
                            pos:self.pos
                    
                    MDToolbar:
                        id:toolbar
                        title: "Copmuter Shortcut"
                        
                        elevation: 10
                        
                        specific_text_color: [0,0,0,1]
                        left_action_items: [['menu', lambda x: nav_drawer.set_state('toggle')]] 
                        
                        md_bg_color: [255/256,193/256,7/256,1]
                        
                        
                                
                                
                        MDIconButton:
                            id: buttonmenu
                            icon: "dots-vertical"
                            pos_hint: {"center_y": .5}
                            on_release: app.menu.open()
                        
                                            
                        
                    MDBottomNavigation:
