#: import ScrollEffect kivy.effects.scroll.ScrollEffect
<NoteBook>:
    name:'NoteBook' 
    BoxLayout:
        orientation: 'vertical'
        canvas.before:
            Color:
                rgba:[1,1,1,1]
            Rectangle:
                
                size:self.size
                pos:self.pos
        
        MDToolbar:
            id:tbaar
            title: "Copmuter Shortcut"
            elevation: 10
            
            specific_text_color: [0,0,0,1]
            left_action_items: [['arrow-left', lambda x: root.blogin()]]  
            
            md_bg_color: [255/256,193/256,7/256,1]
            MDIconButton:
                id: buttonmenu
                icon: "dots-vertical"
                pos_hint: {"center_y": .5}
                on_release: app.menu.open()
        AnchorLayout:
            id:karan
            BoxLayout:
                id:bharat
                orientation:"vertical"
                size_hint_y:None
                height:"100dp"
                size_hint_x:None
                pos_hint: {'center_x': .5, 'center_y': .5}
                MDSpinner:
                    id:bharatss
                    size_hint: None, None
                    size: dp(46), dp(46)
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    active: True
                Label:
                    size_hint_y:None
                    height:"10dp"
                MDLabel:
                    id: check
                    text:"Loading..."
                    size_hint: None, None
                    size: dp(75), dp(40)
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    markup:True
                    bold:True
                    halign:"center"
               
            
            
            
            
           
                                
        

        
            

            
        
                